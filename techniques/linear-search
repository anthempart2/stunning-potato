class Main {
    public static void main(String[] args) {
        int[] arr = {9, 10, 3, 4, 5, 8, 7, 1, 2, 6};
        
        int target =1;
        
        for(int i=0; i<arr.length; i++){
            if(arr[i] == target){
                System.out.println("Found 1 at index: "+i);
                return;
            }
        }
    }
}
