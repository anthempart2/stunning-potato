class program1{
    public static void main(String args[]){
        int[] arr = {1,0,3,0,5};
        //output = {1,3,5,0,0}

        int j =0;
        for(int i=0; i<arr.length; i++){
            if(arr[i]!=0){
                int temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
                j++;
            }
        }

        for(int m=0; m<arr.length;m++){
            System.out.print(arr[m]+ " ");
        }
    }
}


