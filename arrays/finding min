class program1{
    public static void main(String args[]){
        int[] arr = {1,2,3,4,5};

        int i=1; 
        int min= arr[0];
        while(i<arr.length){
            if(arr[i]< min){
                min = arr[i];
            }
            i++;
        }

        System.out.println(min);
    }
}
